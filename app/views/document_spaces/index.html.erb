<p id="notice"><%= notice %></p>

<h1>Document Spaces</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @document_spaces.each do |document_space| %>
      <tr>
        <td><%= link_to 'Show', document_space %></td>
        <td><%= link_to 'Edit', edit_document_space_path(document_space) %></td>
        <td><%= link_to 'Destroy', document_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document Space', new_document_space_path %>
